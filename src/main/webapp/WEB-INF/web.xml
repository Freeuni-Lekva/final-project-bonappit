<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>listeners.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>createAccount</servlet-name>
    <servlet-class>listeners.CreateUserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>createAdmin</servlet-name>
    <servlet-class>listeners.CreateRestaurantServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>adminRegistration</servlet-name>
    <jsp-file>/WEB-INF/login/adminRegistration.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>restaurantPage</servlet-name>
    <jsp-file>/WEB-INF/welcomePage/restaurantPage.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>chatPge</servlet-name>
    <jsp-file>/WEB-INF/welcomePage/chatPage.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>menuServlet</servlet-name>
    <servlet-class>listeners.MenuServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>manageMenuServlet</servlet-name>
    <servlet-class>listeners.ManageMenuServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>createAccount</servlet-name>
    <url-pattern>/createAccount</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>createAdmin</servlet-name>
    <url-pattern>/createAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>adminRegistration</servlet-name>
    <url-pattern>/adminRegistration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>restaurantPage</servlet-name>
    <url-pattern>/restaurantPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>chatPge</servlet-name>
    <url-pattern>/chatPge</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>menuServlet</servlet-name>
    <url-pattern>/menuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>manageMenuServlet</servlet-name>
    <url-pattern>/manageMenuServlet</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>/WEB-INF/login/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
